{ name: "TAILEB MOHAMED", number: "031590001168", company: "EURL UNIVERSAL COLOR", gender: "Masculin", birthDate: "2003-07-11", position: "Agent Polyvalent", service: "Production (Pr√©paration)", direction: "Production", workplace: "", hireDate: "2025-05-29", contractStart: "2025-05-29", contractEnd: "2026-05-28", alert: "358", trialPeriod: "45928", exitDate: "1900-01-00", exitReason: "", csp: "Ex√©cutant" },
