    const employeeInput = document.getElementById('employeeInput');
    const suggestionsDiv = document.getElementById('suggestions');
    const resultDiv = document.getElementById('result');
    let currentSuggestions = [];
    let selectedSuggestionIndex = -1;

    // Function to normalize strings for search (lowercase, no diacritics)
    function normalizeString(str) {
      if (str === null || str === undefined) {
        return '';
      }
      return String(str).toLowerCase().normalize("NFD").replace(/[\u0300-\u036f]/g, "");
    }

    // Function to get the CSS class for company branding
    function getCompanyClass(companyName) {
      if (!companyName) return '';
      const normalizedCompany = normalizeString(companyName);
      if (normalizedCompany.includes("sarl pigma color")) {
        return "sarl-pigma-color";
      } else if (normalizedCompany.includes("eurl universal color")) {
        return "eurl-universal-color";
      } else if (normalizedCompany.includes("eurl veinixia color")) {
        return "eurl-veinixia-color";
      }
      return "autre"; // Default or other company class
    }

    // Function to display a single employee card
    function displayEmployeeCard(employee, matchType = '') {
      const companyClass = getCompanyClass(employee.company);
      const genderClass = employee.gender === 'Masculin' ? 'gender-male' : (employee.gender === 'F√©minin' ? 'gender-female' : '');

      const fullName = employee.name || 'Non renseign√©';
      const nameParts = fullName.split(' ');
      const nom = nameParts.length > 1 ? nameParts[0] : fullName;
      const prenom = nameParts.length > 1 ? nameParts.slice(1).join(' ') : '';

      return `
        <div class="employee-card ${companyClass}">
          <div class="employee-header">
            <div class="employee-name"><i class="fas fa-user-circle"></i> ${fullName}</div>
            ${employee.number ? `<div class="employee-number"><i class="fas fa-id-card"></i> SS: ${employee.number}</div>` : ''}
          </div>

          <div class="employee-details-card">
            <h3><i class="fas fa-info-circle"></i> Informations Personnelles</h3>
            <div class="personal-info-section">
              ${nom ? `<div class="detail-item">
                <div class="detail-label"><i class="fas fa-signature"></i> Nom</div>
                <div class="detail-value">${nom}</div>
              </div>` : ''}
              ${prenom ? `<div class="detail-item">
                <div class="detail-label"><i class="fas fa-signature"></i> Pr√©nom</div>
                <div class="detail-value">${prenom}</div>
              </div>` : ''}
              ${employee.gender ? `<div class="detail-item">
                <div class="detail-label"><i class="fas fa-venus-mars"></i> Sexe</div>
                <div class="detail-value ${genderClass}">${employee.gender}</div>
              </div>` : ''}
              ${employee.birthDate && employee.birthDate !== "1900-01-00" ? `<div class="detail-item">
                <div class="detail-label"><i class="fas fa-calendar-alt"></i> Date de Naissance</div>
                <div class="detail-value">${employee.birthDate}</div>
              </div>` : ''}
            </div>

            <h3><i class="fas fa-briefcase"></i> Informations Professionnelles</h3>
            <div class="professional-info-section">
              ${employee.position ? `<div class="detail-item">
                <div class="detail-label"><i class="fas fa-briefcase"></i> Poste</div>
                <div class="detail-value emphasized">${employee.position}</div>
              </div>` : ''}
              ${employee.service ? `<div class="detail-item">
                <div class="detail-label"><i class="fas fa-cogs"></i> Service</div>
                <div class="detail-value">${employee.service}</div>
              </div>` : ''}
              ${employee.direction ? `<div class="detail-item">
                <div class="detail-label"><i class="fas fa-sitemap"></i> Direction</div>
                <div class="detail-value">${employee.direction}</div>
              </div>` : ''}
              ${employee.workplace ? `<div class="detail-item">
                <div class="detail-label"><i class="fas fa-building"></i> Lieu de Travail</div>
                <div class="detail-value">${employee.workplace}</div>
              </div>` : ''}
              ${employee.hireDate && employee.hireDate !== "1900-01-00" ? `<div class="detail-item">
                <div class="detail-label"><i class="fas fa-calendar-plus"></i> Date d'Embauche</div>
                <div class="detail-value">${employee.hireDate}</div>
              </div>` : ''}
              ${employee.contractStart && employee.contractStart !== "1900-01-00" ? `<div class="detail-item">
                <div class="detail-label"><i class="fas fa-file-contract"></i> D√©but du Contrat</div>
                <div class="detail-value">${employee.contractStart}</div>
              </div>` : ''}
              ${employee.contractEnd && employee.contractEnd !== "1900-01-00" ? `<div class="detail-item">
                <div class="detail-label"><i class="fas fa-file-contract"></i> Fin du Contrat</div>
                <div class="detail-value">${employee.contractEnd === "Indeterminie" ? 'Ind√©termin√©e' : employee.contractEnd}</div>
              </div>` : ''}
              ${employee.alert && employee.alert.trim() !== "" ? `<div class="detail-item">
                <div class="detail-label"><i class="fas fa-exclamation-triangle"></i> Alerte</div>
                <div class="detail-value">${employee.alert}</div>
              </div>` : ''}
              ${employee.trialPeriod && employee.trialPeriod.trim() !== "" ? `<div class="detail-item">
                <div class="detail-label"><i class="fas fa-hourglass-half"></i> P√©riode d'essai</div>
                <div class="detail-value">${employee.trialPeriod}</div>
              </div>` : ''}
              ${employee.exitDate && employee.exitDate !== "1900-01-00" ? `<div class="detail-item">
                <div class="detail-label"><i class="fas fa-calendar-minus"></i> Date de Sortie</div>
                <div class="detail-value">${employee.exitDate}</div>
              </div>` : ''}
              ${employee.exitReason && employee.exitReason.trim() !== "" ? `<div class="detail-item">
                <div class="detail-label"><i class="fas fa-sign-out-alt"></i> Motif de Sortie</div>
                <div class="detail-value">${employee.exitReason}</div>
              </div>` : ''}
              ${employee.csp ? `<div class="detail-item">
                <div class="detail-label"><i class="fas fa-users-cog"></i> CSP</div>
                <div class="detail-value">${employee.csp}</div>
              </div>` : ''}
              ${employee.company ? `<div class="detail-item">
                <div class="detail-label"><i class="fas fa-building"></i> Entreprise</div>
                <div class="detail-value"><span class="company-${companyClass}">${employee.company}</span></div>
              </div>` : ''}
            </div>
          </div>
          ${matchType ? `<div style="font-size: 0.8rem; color: #6b7280; margin-top: 1rem; text-align: left;">Type de correspondance: ${matchType}</div>` : ''}
        </div>
      `;
    }

    // Function to perform search and display results
    function searchEmployee(queryOrEmployee) {
      resultDiv.innerHTML = ''; // Clear previous results
      suggestionsDiv.style.display = 'none'; // Hide suggestions

      let employeesToDisplay = [];

      if (typeof queryOrEmployee === 'object') {
        // If an employee object is passed (from autocomplete click/enter)
        employeesToDisplay.push(queryOrEmployee);
        employeeInput.value = queryOrEmployee.name; // Set input field to selected employee's name
      } else {
        // Manual search or search initiated from input
        const query = normalizeString(queryOrEmployee);

        if (query.length === 0) {
          resultDiv.innerHTML = '<div class="no-result">Veuillez saisir un terme de recherche.</div>';
          return;
        }

        // Exact match by name
        let exactMatches = employeesData.filter(employee => normalizeString(employee.name) === query);
        if (exactMatches.length > 0) {
          employeesToDisplay = exactMatches.map(emp => ({ ...emp, matchType: 'Nom exact' }));
        } else {
          // Partial matches by name, position, company, service, workplace, or SS number
          const partialMatches = employeesData.filter(employee =>
            normalizeString(employee.name).includes(query) ||
            normalizeString(employee.position).includes(query) ||
            normalizeString(employee.company).includes(query) ||
            normalizeString(employee.service).includes(query) ||
            normalizeString(employee.workplace).includes(query) ||
            normalizeString(employee.number).includes(query)
          );
          employeesToDisplay = partialMatches.map(emp => ({ ...emp, matchType: 'Correspondance partielle' }));
        }
      }

      if (employeesToDisplay.length > 0) {
        employeesToDisplay.forEach(employee => {
          resultDiv.innerHTML += displayEmployeeCard(employee, employee.matchType);
        });
      } else {
        resultDiv.innerHTML = '<div class="no-result">Aucun employ√© trouv√© pour votre recherche.</div>';
      }
    }

    // Function to perform search when the "Rechercher" button is clicked
    function searchEmployeeManual() {
      const query = employeeInput.value;
      searchEmployee(query);
    }

    // Function to clear search and results
    function clearSearch() {
      employeeInput.value = '';
      resultDiv.innerHTML = '';
      suggestionsDiv.innerHTML = '';
      suggestionsDiv.style.display = 'none';
      selectedSuggestionIndex = -1;
    }

    // Function to display statistics
    function showStatistics() {
      resultDiv.innerHTML = ''; // Clear search results

      const totalEmployees = employeesData.length;

      // Gender distribution
      const genderCounts = employeesData.reduce((acc, employee) => {
        acc[employee.gender] = (acc[employee.gender] || 0) + 1;
        return acc;
      }, {});

      // Company distribution
      const companyCounts = employeesData.reduce((acc, employee) => {
        acc[employee.company] = (acc[employee.company] || 0) + 1;
        return acc;
      }, {});

      // Position distribution (top 5 for brevity, or all if few)
      const positionCounts = employeesData.reduce((acc, employee) => {
        acc[employee.position] = (acc[employee.position] || 0) + 1;
        return acc;
      }, {});
      const sortedPositions = Object.entries(positionCounts).sort(([, a], [, b]) => b - a);
      const topPositions = sortedPositions.slice(0, 5); // Display top 5 positions

      let statsHtml = `
        <div class="stats-container">
          <div class="stat-card">
            <div class="stat-number">${totalEmployees}</div>
            <div class="stat-label">Total Employ√©s</div>
          </div>
          <div class="stat-card">
            <div class="stat-number">${genderCounts['Masculin'] || 0}</div>
            <div class="stat-label">Hommes</div>
          </div>
          <div class="stat-card">
            <div class="stat-number">${genderCounts['F√©minin'] || 0}</div>
            <div class="stat-label">Femmes</div>
          </div>
        </div>
      `;

      // Gender Chart
      if (Object.keys(genderCounts).length > 0) {
          statsHtml += `
            <div class="chart-container">
                <div class="chart-title">R√©partition par Sexe</div>
                <div class="bar-chart">
                    ${Object.entries(genderCounts).map(([gender, count]) => {
                        const percentage = ((count / totalEmployees) * 100).toFixed(1);
                        const barClass = gender === 'Masculin' ? 'gender-male-bg' : 'gender-female-bg';
                        return `
                            <div class="bar-item">
                                <span class="bar-label">${gender}</span>
                                <div class="bar-graph">
                                    <div class="bar-fill ${barClass}" style="width: ${percentage}%">${percentage}%</div>
                                </div>
                            </div>
                        `;
                    }).join('')}
                </div>
            </div>
          `;
      }


      // Company Chart
      if (Object.keys(companyCounts).length > 0) {
          statsHtml += `
            <div class="chart-container">
                <div class="chart-title">R√©partition par Entreprise</div>
                <div class="bar-chart">
                    ${Object.entries(companyCounts).map(([company, count]) => {
                        const percentage = ((count / totalEmployees) * 100).toFixed(1);
                        const companyClass = getCompanyClass(company); // Re-use company class logic
                        const barClass = `company-${companyClass}-bg`; // Use the specific background class
                        return `
                            <div class="bar-item">
                                <span class="bar-label">${company}</span>
                                <div class="bar-graph">
                                    <div class="bar-fill ${barClass}" style="width: ${percentage}%">${percentage}%</div>
                                </div>
                            </div>
                        `;
                    }).join('')}
                </div>
            </div>
          `;
      }

      // Top Positions Chart
      if (topPositions.length > 0) {
          statsHtml += `
            <div class="chart-container">
                <div class="chart-title">Top 5 des Postes</div>
                <div class="bar-chart">
                    ${topPositions.map(([position, count]) => {
                        const percentage = ((count / totalEmployees) * 100).toFixed(1);
                        // You can define specific background colors for positions if needed
                        const barClass = 'company-sarl-pigma-color-bg'; // Example: using a default for now
                        return `
                            <div class="bar-item">
                                <span class="bar-label">${position}</span>
                                <div class="bar-graph">
                                    <div class="bar-fill ${barClass}" style="width: ${percentage}%">${percentage}%</div>
                                </div>
                            </div>
                        `;
                    }).join('')}
                </div>
            </div>
          `;
      }

      resultDiv.innerHTML = statsHtml;
    }


    // Autocomplete functionality
    employeeInput.addEventListener('input', function() {
      const query = normalizeString(this.value);
      suggestionsDiv.innerHTML = '';
      suggestionsDiv.style.display = 'none';
      currentSuggestions = [];
      selectedSuggestionIndex = -1;

      if (query.length < 2) { // Show suggestions after at least 2 characters
        return;
      }

      const matches = employeesData.filter(employee =>
        normalizeString(employee.name).includes(query) ||
        normalizeString(employee.position).includes(query) ||
        normalizeString(employee.company).includes(query) ||
        normalizeString(employee.number).includes(query)
      );

      if (matches.length > 0) {
        currentSuggestions = matches;
        matches.forEach((employee, index) => {
          const suggestionItem = document.createElement('div');
          suggestionItem.classList.add('suggestion-item');
          suggestionItem.dataset.index = index; // Store index for selection
          suggestionItem.innerHTML = `
            <span class="suggestion-name">${employee.name}</span>
            <div class="suggestion-details">
              <span class="suggestion-profession">${employee.position}</span>
              <span class="suggestion-entreprise">${employee.company}</span>
            </div>
          `;
          suggestionsDiv.appendChild(suggestionItem);
        });
        suggestionsDiv.style.display = 'block';
      }
    });

    // Keyboard navigation for suggestions
    employeeInput.addEventListener("keydown", function(e) {
      const items = suggestionsDiv.querySelectorAll(".suggestion-item");
      if (items.length === 0) return;

      switch (e.key) {
        case "ArrowUp":
          e.preventDefault(); // Prevent cursor movement in input
          if (selectedSuggestionIndex > 0) {
            items[selectedSuggestionIndex].classList.remove("selected");
            selectedSuggestionIndex--;
            items[selectedSuggestionIndex].classList.add("selected");
            // Scroll to view
            items[selectedSuggestionIndex].scrollIntoView({ block: "nearest" });
          }
          break;

        case "ArrowDown":
          e.preventDefault(); // Prevent cursor movement in input
          if (selectedSuggestionIndex < items.length - 1) {
            if (selectedSuggestionIndex > -1) {
              items[selectedSuggestionIndex].classList.remove("selected");
            }
            selectedSuggestionIndex++;
            items[selectedSuggestionIndex].classList.add("selected");
            // Scroll to view
            items[selectedSuggestionIndex].scrollIntoView({ block: "nearest" });
          }
          break;

        case "Enter":
          e.preventDefault(); // Prevent form submission
          if (selectedSuggestionIndex > -1) {
            const selectedEmployee = currentSuggestions[selectedSuggestionIndex];
            // Call searchEmployee with the selected employee object
            searchEmployee(selectedEmployee);
          } else {
            // If Enter is pressed without selecting a suggestion, perform manual search
            searchEmployeeManual();
          }
          break;

        case "Escape":
          suggestionsDiv.style.display = "none";
          selectedSuggestionIndex = -1;
          document.getElementById("result").innerHTML = ""; // Clear results on escape
          break;
      }
    });

    // Click event for selecting a suggestion
    document.getElementById("suggestions").addEventListener("click", function(event) {
        const item = event.target.closest('.suggestion-item');
        if (item) {
            const index = parseInt(item.dataset.index);
            const selectedEmployee = currentSuggestions[index];
            // Call searchEmployee with the selected employee object
            searchEmployee(selectedEmployee);
        }
    });

    // Hide suggestions when clicking outside
    document.addEventListener("click", function(event) {
      if (!event.target.closest('.autocomplete-container') && !event.target.closest('.result')) {
        document.getElementById("suggestions").style.display = "none";
      }
    });

    // Initial clear results on page load
    document.addEventListener("DOMContentLoaded", () => {
        document.getElementById("result").innerHTML = "";
    });
  </script>
</body>
</html>
